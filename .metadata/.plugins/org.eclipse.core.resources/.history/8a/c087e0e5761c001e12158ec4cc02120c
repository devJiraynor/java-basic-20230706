package chapter1;

public class ConsoleIO {

	public static void main(String[] args) {
		// Java의 콘솔을 이용한 입출력
		
		// 출력 
		// System.out 객체의 println() 또는 print() 메서드를 사용
		// println() : 줄바꿈이 이뤄지는 출력
		// print() : 줄바꿈이 이뤄지지 않는 출력
		System.out.print("출력입니다.");
		System.out.println("출력입니다.");
		
		// 입력
		// 
	}

}

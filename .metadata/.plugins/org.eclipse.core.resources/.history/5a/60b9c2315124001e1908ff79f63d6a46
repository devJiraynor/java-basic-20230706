	void teaching() {
		System.out.println(gradeClass + "에서 수업을 한다.");
	}
}

class Police {
	String name;
	int age;
	String address;
	String position;
	String area;
	
	void eat(String food) {
		System.out.println(food + "을/를 먹는다.");
	}
	
	void patrol() {
		System.out.println(area + "를 순찰합니다.");
	}
}

